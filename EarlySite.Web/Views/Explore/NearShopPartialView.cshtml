@using EarlySite.Model.Common;
@using EarlySite.Model.Show;

@model IList<Shop>

@{
    if (Model != null && Model.Count > 0)
    {
        foreach (Shop shopitem in Model)
        {
            <div class="row ShopBoxItem">
                <div class="col-sm-4">
                    <div class="shopInfoBox" id="shopbox_@shopitem.ShopId" data-shopId="@shopitem.ShopId">
                        <h4>@shopitem.ShopName</h4>
                        <div class="shopInfoBox-title">店铺地址</div>
                        <div class="shopInfoBox-content">@shopitem.ShopAddress</div>
                        <div class="shopInfoBox-title">店铺描述</div>
                        <div class="shopInfoBox-des" title="@shopitem.Description">@shopitem.Description</div>
                    </div>

                </div>
                <div class="col-sm-8">
                    <ul class="shopboxlist">
                        <li class="shopboxlist-item">
                            <div class="shopboxlist-item_box">
                                <div class="shopboxlist-cover">
                                    <a class="shopboxlist-a" href="javascript:alert(1)">
                                        <img class="shopboxlist-pic" src="data:image/png;base64," alt="xxx" />
                                    </a>
                                </div>
                                <div class="boxlist-title">
                                    <span class="boxlist-title-txt">
                                        <a class="boxlist-a" href="javascript:alert(1)" title="">单品名称</a>
                                    </span>
                                </div>
                                <div class="shopboxlist-other">川菜</div>
                                <div class="shopboxlist-other">￥25.00</div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        }
    }


}




<div class="ShopBoxBottom">
    <a href="javascript:alert(1)">点此加载更多...</a>
</div>

<script type="text/javascript">
    $(function () {

        toast("loading");
    });


</script>
